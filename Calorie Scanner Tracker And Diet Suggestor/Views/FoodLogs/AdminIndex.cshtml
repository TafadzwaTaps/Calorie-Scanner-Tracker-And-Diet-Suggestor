@model IEnumerable<Calorie_Scanner_Tracker_And_Diet_Suggestor.Models.FoodLog>

<h2>Admin - Food Logs</h2>
<table class="table">
    <thead>
        <tr>
            <th>User</th>
            <th>Meal</th>
            <th>Date Logged</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td>@log.User.Username</td>
                <td>@log.Meal.Name</td>
                <td>@log.DateLogged.ToString("yyyy-MM-dd HH:mm")</td>
                <td>
                    <a asp-action="Delete" asp-route-id="@log.Id" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
